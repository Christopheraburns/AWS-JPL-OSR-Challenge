          },
          "robotSoftwareSuite": {
            "name": "ROS",
            "version": "Melodic"
          }
        }
      }
    },
    {
      "id": "training-evaluation-sim",
      "name": "training-evaluation-sim",
      "type": "simulation",
      "cfg": {
        "simulation": {
          "maxJobDurationInSeconds": 1800,
          "failureBehavior": "Continue",
          "iamRole": "[YOUR_IAM_ROLE]",
          "outputLocation": "burnsca-training-output",
          "vpcConfig": {
